{

  "plugins": ["node_modules/jsdoc-babel"],
  "source": {
    "includePattern": ".+.(j|t)s(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "babel": {
    "babelrc": false,
    "extensions": ["js", "jsx", "ts", "tsx"],
    "presets": [
      "@babel/preset-typescript"
    ]
  },

  "opts": {
    "recursive": true,
    "encoding": "utf8",
    "destination": "docs"
  }
}
